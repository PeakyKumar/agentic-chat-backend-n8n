    {
      "parameters": {
        "jsCode": "const sessionId = $json.session_id;\nconst userMessage = $json.user_message;\n\nconst memory = $getWorkflowStaticData('global');\n\n// Initialize session memory if not present\nif (!memory[sessionId]) {\n  memory[sessionId] = [];\n}\n\n// Add user message to memory\nmemory[sessionId].push({\n  role: 'user',\n  content: userMessage,\n});\n\n// Build conversation text for LLM\nconst conversation = memory[sessionId]\n  .map(m => `${m.role.toUpperCase()}: ${m.content}`)\n  .join('\\n');\n\nreturn {\n  session_id: sessionId,\n  conversation,\n};\n"
      },
